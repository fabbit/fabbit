<div class="row">
  <h1><%= @folder %></h1>
  <h3><%= link_to "Back", navigate_path(parent_link(@parent)) %></h3>
</div>
<div class="row">
  <% @contents.each do |content| %>
    <p>
      <%= link_to content[:content], content[:link] %>
      <b><%= " >" if content[:is_dir] %></b>
    </p>
  <% end %>
</div>
